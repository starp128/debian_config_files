[core]
	editor = emacs -nw
[alias]
	lg = log --decorate --oneline --graph
	logd = log --decorate --pretty=fuller
	logdo = log --decorate --oneline
	logdog = log --decorate --oneline --graph
	logdogb = log --decorate --oneline --graph --boundary
	st = status
	br = branch
	b = branch
[color]
	diff = auto
	status = auto
	branch = auto
	log = auto
	ui = auto
